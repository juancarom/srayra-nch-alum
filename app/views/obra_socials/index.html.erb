<h1>Listing obra_socials</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @obra_socials.each do |obra_social| %>
  <tr>
    <td><%= obra_social.nombre %></td>
    <td><%= link_to 'Show', obra_social %></td>
    <td><%= link_to 'Edit', edit_obra_social_path(obra_social) %></td>
    <td><%= link_to 'Destroy', obra_social, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

